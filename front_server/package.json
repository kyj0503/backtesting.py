{
  "name": "backtesting-frontend",
  "version": "1.0.0",
  "description": "Professional backtesting dashboard built with React, TypeScript, Bootstrap, and Recharts for visualizing trading strategy performance",
  "private": true,
  "type": "module",
  "keywords": [
    "backtesting",
    "trading",
    "finance",
    "react",
    "typescript",
    "charts",
    "dashboard",
    "bootstrap",
    "recharts",
    "technical-analysis"
  ],
  "author": "Backtesting Team",
  "license": "MIT",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/backtesting-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/backtesting-frontend/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bootstrap": "^5.3.6",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.2.0",
    "recharts": "^2.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview --host",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
